<%- include("include/header", { titulo: 'Health Road - Registro Usuario' }) %>

<body class="clinic_version">
   <div id="preloader">
      <img class="preloader" src="images/loaders/heart-loading2.gif" alt="">
   </div>
   <header>

   </header>
   <div id="getintouch" class="section wb wow fadeIn" style="padding-bottom:0;">
      <div class="container">
         <div class="heading">
            <span class="icon-logo"><img src="images/icon-logo.png" alt="#"></span>
            <h2>Registrar Usuario</h2>
         </div>
      </div>
      <div class="container forms">
         <div class="contact-section">
            <div class="form-contant">
               <form id="ajax-contact" action="" method="post">
                  <div class="row">
                     <div class="col-md-6">
                        <div class="form-group in_name">
                           <label> <i class="far fa-user"></i> Nombre </label>
                           <input type="text" class="form-control" placeholder="Nombre" required="required">
                        </div>
                     </div>
                     <div class="col-md-6">
                        <div class="form-group in_email">
                           <label> Apellido</label>
                           <input type="text" class="form-control" placeholder="Apellido" required="required">
                        </div>
                     </div>
                     <div class="col-md-12">
                        <div class="form-group in_email">
                           <label> <i class="fas fa-envelope-open-text"></i> Email</label>
                           <input type="email" class="form-control" placeholder="Email" required="required">
                        </div>
                     </div>
                     <div class="col-md-12">
                        <div class="form-group in_email">
                           <label> <i class="far fa-phone-rotary"></i> Telefono</label>
                           <input type="text" class="form-control" placeholder="Telefono" required="required">
                        </div>
                     </div>
                     <div class="col-md-12">
                        <div class="form-group in_email">
                           <label> <i class="far fa-lock-alt"></i> Contraseña</label>
                           <input type="password" class="form-control" placeholder="Contraseña" required="required">
                        </div>
                     </div>
                     <div class="col-md-12">
                        <div class="form-group in_email">
                           <label> <i class="far fa-user-tag"></i> Rol</label>
                           <select name="Rol">
                              
                           </select>
                        </div>
                        <div class="actions">
                           <input type="submit" value="Registrar" name="submit" id="submitButton" class="btn small"
                              title="Submit Your Message!">
                        </div>
                     </div>
                  </div>

               </form>
            </div>

         </div>

      </div>

   </div>

   <%- include("include/scripts") %>


</body>

</html>